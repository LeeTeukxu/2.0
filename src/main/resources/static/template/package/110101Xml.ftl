<?xml version="1.0" encoding="UTF-8"?>
<发明专利请求书>
    <收到日期/>
    <申请号/>
    <分案提交日/>
    <申请日/>
    <费减审批/>
    <向外申请审批/>
    <挂号号码/>
    <发明名称>${famingmc}</发明名称>
    <发明英文名称/>
    <内部编号>${nbbh}</内部编号>
    <发明人>
        <第一发明人>
            <姓名>${firstInventor}</姓名>
            <请求不公开>
                <附加>0</附加>
            </请求不公开>
            <国籍>${firstInventorCountry}</国籍>
            <身份证号>${firstInventorIDCode}</身份证号>
            <英文姓名/>
        </第一发明人>
        <#list  inventors as inventor>
            <其他发明人 顺序="${inventor_index+1}">
                <姓名>${inventor.name}</姓名>
                <请求不公开>
                    <附加>${inventor.notOpen?then(1,0)}</附加>
                </请求不公开>
                <英文姓名/>
            </其他发明人>
        </#list>
    </发明人>
    <申请人>
<#list applyMans as applyMan>
    <<#if applyMan_index==0>第一申请人<#else>其他申请人 顺序="${applyMan_index}"</#if>>
    <#if applyMan??>
        <姓名或名称>${applyMan.name}</姓名或名称>
        <申请人类型>${applyMan.type}</申请人类型>
        <用户代码/>
        <个人身份证号或组织机构代码>${applyMan.idCode}</个人身份证号或组织机构代码>
        <电子邮箱>${applyMan.email}</电子邮箱>
        <国籍或总部所在地>${applyMan.country}</国籍或总部所在地>
        <经营所在地或营业所在地>${applyMan.address}</经营所在地或营业所在地>
        <省自治区直辖市名称>${applyMan.province}</省自治区直辖市名称>
        <市县名称>${applyMan.city}</市县名称>
        <城区乡街道门牌号>${applyMan.street}</城区乡街道门牌号>
        <邮政编码>${applyMan.postCode}</邮政编码>
        <电话/>
        <英文名称/>
        <英文地址/>
        <费减备案>
            <附加>${applyMan.requestFj?then(1,0)}</附加>
        </费减备案>
    <#else>
        <姓名或名称/>
        <用户代码/>
        <申请人类型/>
        <国籍或总部所在地/>
        <经营所在地或营业所在地/>
        <省自治区直辖市名称/>
        <市县名称/>
        <城区乡街道门牌号/>
        <邮政编码/>
        <电话/>
        <英文名称/>
        <英文地址/>
        <个人身份证号或组织机构代码/>
        <费减备案>
            <附加>0</附加>
        </费减备案>
    </#if>
    <#if applyMan_index==0></第一申请人><#else> </其他申请人></#if>
</#list>
    </申请人>
    <代表人/>
    <联系人>
        <姓名/>
        <电话/>
        <电子邮箱/>
        <邮政编码/>
        <省自治区直辖市名称/>
        <市县名称/>
        <城区乡街道门牌号/>
    </联系人>
    <专利代理机构>
        <名称>长沙智德知识产权代理事务所（普通合伙）</名称>
        <代码>43207</代码>
        <声明已经与申请人签订了专利代理委托书且本表中的信息与委托书中相应信息一致>
            <附加>1</附加>
        </声明已经与申请人签订了专利代理委托书且本表中的信息与委托书中相应信息一致>
        <#list agents as agent>
        <代理人 顺序="${agent_index+1}">
            <#if agent??>
                <姓名>${agent.name}</姓名>
                <工作证号>${agent.code}</工作证号>
                <电话>${agent.phone}</电话>
            <#else >
                <姓名/>
                <工作证号/>
                <电话/>
            </#if>
        </代理人>
        </#list>
    </专利代理机构>
    <分案申请>
        <原申请号/>
        <针对的分案申请号/>
        <原申请日>
            <年/>
            <月/>
            <日/>
        </原申请日>
    </分案申请>
    <生物材料样品信息>
        <生物材料样品 顺序="1">
            <保藏单位/>
            <地址/>
            <保藏日期>
                <年/>
                <月/>
                <日/>
            </保藏日期>
            <保藏编号/>
            <分类命名/>
            <存活情况>
                <存活>
                    <附加>0</附加>
                </存活>
                <未存活>
                    <附加>0</附加>
                </未存活>
            </存活情况>
        </生物材料样品>
    </生物材料样品信息>
    <序列表>
        <本申请涉及核苷酸或氨基酸序列表>
            <附加>0</附加>
        </本申请涉及核苷酸或氨基酸序列表>
    </序列表>
    <要求优先权声明>
        <在先申请 顺序="1">
            <在先申请国别或地区/>
            <在先申请日/>
            <在先申请号/>
        </在先申请>
        <在先申请 顺序="2">
            <在先申请日/>
            <在先申请号/>
            <在先申请国别或地区/>
        </在先申请>
        <在先申请 顺序="3">
            <在先申请日/>
            <在先申请号/>
            <在先申请国别或地区/>
        </在先申请>
        <在先申请 顺序="4">
            <在先申请日/>
            <在先申请号/>
            <在先申请国别或地区/>
        </在先申请>
        <在先申请 顺序="5">
            <在先申请日/>
            <在先申请号/>
            <在先申请国别或地区/>
        </在先申请>
        <在先申请 顺序="6">
            <在先申请日/>
            <在先申请号/>
            <在先申请国别或地区/>
        </在先申请>
        <在先申请 顺序="7">
            <在先申请日/>
            <在先申请号/>
            <在先申请国别或地区/>
        </在先申请>
        <在先申请 顺序="8">
            <在先申请日/>
            <在先申请号/>
            <在先申请国别或地区/>
        </在先申请>
    </要求优先权声明>
    <不丧失新颖性申明颖性申明>
        <已在中国政府主办或承认的国际展览会上首次展出>
            <附加>0</附加>
        </已在中国政府主办或承认的国际展览会上首次展出>
        <已在规定的学术会议或技术会议上首次发表>
            <附加>0</附加>
        </已在规定的学术会议或技术会议上首次发表>
        <他人未经申请人同意而泄露其内容>
            <附加>0</附加>
        </他人未经申请人同意而泄露其内容>
    </不丧失新颖性申明颖性申明>
    <提前公开>
        <请求早日公布该专利申请>
            <附加>${advanceApply?then(1,0)}</附加>
        </请求早日公布该专利申请>
    </提前公开>
    <遗传资源>
        <本专利申请涉及的发明创造是依赖于遗传资源完成的>
            <附加>0</附加>
        </本专利申请涉及的发明创造是依赖于遗传资源完成的>
    </遗传资源>
    <同日申请>
        <声明本发明在同日申请实用新型专利>
            <附加>${sameApply?then(1,0)}</附加>
        </声明本发明在同日申请实用新型专利>
    </同日申请>
    <摘要附图>
        <图号>${digistImage}</图号>
    </摘要附图>
    <申请文件清单>
        <申请文件信息>
            <#list files as file>
                <申请文件 顺序="${file_index+1}">
                    <文件名称>${file.type}</文件名称>
                    <表格代码>${file.code}</表格代码>
                    <页数>${file.pages}</页数>
                </申请文件>
            </#list>
        </申请文件信息>
        <项数>${itemCount}</项数>
    </申请文件清单>
    <附加文件清单>
        <#list addFiles as file>
            <附加文件 顺序="${file_index+1}">
                <文件名称>${file.type}</文件名称>
                <表格代码>${file.code}</表格代码>
                <页数>${file.pages}</页数>
            </附加文件>
        </#list>
        <备案编号/>
    </附加文件清单>
    <申请人或代理机构签章>
        <签章>${agentName}</签章>
        <签章日期>
            <年>${year}</年>
            <月>${month}</月>
            <日>${day}</日>
        </签章日期>
    </申请人或代理机构签章>
    <专利局审批>
        <审核意见/>
        <提交日期>
            <年/>
            <月/>
            <日/>
        </提交日期>
    </专利局审批>
</发明专利请求书>
